	COLOR: #1c5c9a; 
	BACKGROUND: #ecf2f8; 
	BORDER-COLOR: #6398cd;
}
.pages A.prev {
	FONT-WEIGHT: bold;
}
.pages SPAN {
	BACKGROUND: #fff; 
	MARGIN-RIGHT: 0.1em; 
