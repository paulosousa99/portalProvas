	color:#000;
	text-align:center;
	margin:0px;
	padding:0px;
}

p,pre, h1,h2 {
	margin:0px 10px 10px 10px;
}

h1 {
	font-size:14px;
	color:#455B87;
}

h2 {
	font-size:12px;
	color:#EC8711;
}

a {
	color: #13ABFD;
	text-decoration: none;
}

a:hover{
	text-decoration:underline;
}
a:visited{
	color:#13ABFD;
}

hr {
	background-color: #6d4c07; /* ubuntu dark brown */
}

ul {
	margin-left: 5px;
}

img {
	border-top-style: none; 
	border-right-style: none; 
	border-left-style: none; 
	border-bottom-style: none;
	border:0px;
}

img a:link{
	border:0px;
}

pre {
	color: #564b47;
	font-size: 11px;
	background-color:transparent;
	font-family: Courier, Monaco, Monospace;
}

#box {
	width:790px;
	margin: 0px auto;
	padding:0px;
