	button { 
		border:0; 
		cursor:pointer; 
		font-weight:bold; 
		padding:0 20px 0 0; 
		text-align:center; 
		/* IE 7*/
		width:auto; 
		overflow:visible; 

	}
	button span { 
		position:relative; 
		display:block; 
		white-space:nowrap; 
		padding:0 0 0 20px; 
		/* IE 7*/
		margin-top:1px; 
	}

	/*blue buttons*/
	button.submitBtn { 
		background:url(imagens/btn_blue_right.gif) right no-repeat; 
		font-size:1.3em; 
	}
	button.submitBtn span { 
		height:50px; 
		line-height:50px;
		background:url(imagens/btn_blue_left.gif) left no-repeat;
		color:#fff; 
	}
	button.submitBtn:hover {
		background:url(imagens/btn_blue_right_hover.gif) right no-repeat; 
	}
	button.submitBtn:hover span {
		background:url(imagens/btn_blue_left_hover.gif) left no-repeat; 
	}

	/*green buttons*/
	button.gravarBtn { 
		background:url(imagens/btn_green_right.gif) right no-repeat; 
		font-size:1.3em; 
	}
	button.gravarBtn span { 
		height:50px; 
		line-height:50px;
		background:url(imagens/btn_green_left.gif) left no-repeat;
		color:#fff; 
	}
	button.gravarBtn:hover {
		background:url(imagens/btn_green_right_hover.gif) right no-repeat; 
	}
	button.gravarBtn:hover span {
		background:url(imagens/btn_green_left_hover.gif) left no-repeat; 
	}
		
		
