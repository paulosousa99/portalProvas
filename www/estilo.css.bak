/* FORMULARIO DE CADASTRO */

fieldset.cadastro {
	border:none;
	border-top:5px solid #D7E6FF;
}

fieldset.cadastro legend {
	font-size: 14px;
	font-weight: bold;
	color: #000000;
	border:none;
}


/* Info */

	div.info {
		width: 80%;
		margin: 10px auto;
		border: 1px solid #d3e6fe;
		background: #fff url('imagens/info_bg.jpg') repeat-x;
		font-size: 0.9em;
		padding: 5px 5px 15px 5px;
		position: relative;
	}
	
	div.info h2 {
		height: 31px;
		margin: 5px 0 -5px 10px;
		padding: 5px 0 0 0;
		font-size: 1.3em;
		text-indent: 30px;
		background: url('imagens/info_ico.gif') no-repeat;
	}
	
	div.info p, div.attention p {
		text-indent: 0;
	}
	
	/* Atención */

	div.attention {
		width: 80%;
		margin: 10px auto;
		border: 1px solid #f9ffa8;
		background: #fff url('imagens/att_bg.jpg') repeat-x;
		font-size: 0.9em;
		padding: 5px 5px 15px 5px;
		position: relative;
	}
	
	div.attention h2 {
		height: 31px;
		margin: 5px 0 -5px 10px;
		padding: 5px 0 0 0;
		font-size: 1.3em;
		text-indent: 30px;
		background: url('imagens/att_ico.gif') no-repeat;
	}
	
/* Contenido */

	div#contenido {
		width: 80%;
		margin: auto;
		margin-top: 20px;
		margin-bottom: 50px;
		padding: 10px 0 0 0;
		background: #fff url('imagens/cont_bg.jpg') no-repeat left top;
	}
	
	div#contenido h2 {
		font-size: 1.5em;
		margin: 10px 0 0 15px;
		color: #30cffe;
	}





