/* TABELA */
.tabela { 
	font:12px Arial, Helvetica, sans-serif; 
	width:100%; 
	margin:0 0 10px 0; 
}

table { 
	font-size:12px;
	font-family:Arial, Helvetica, sans-serif;
}

.tabela caption {
	background-color:#AD6D5A;
	color:#FFF;
	text-transform:uppercase;
	font-weight:bold;
	font-size:1.2em;
	border:1px solid #000;
}

.tabela td{
	height:25px;
}

.tabela thead td {
	text-transform:uppercase;
	background-image:url(imagens/tabela-bg-thead.jpg);
	/*background-image:url(imagens/tabela_titulo_fundo.gif);*/
	background-repeat:repeat-x;
	border:1px solid #406EA1;
	color:#404040;
	font-weight:bold;
	text-align:left;
}

.tabela thead a {
	color:#404040;
}

.tabela thead th a:visited {
	color:#FFF;
}

.tabela tfoot td {
	background-image:url(imagens/tabela_titulo_fundo.gif);
	background-repeat:repeat-x;
	color:#FFFFFF;
	text-transform:uppercase;
	font-size:10px;
 }
.tabela TBODY TR {
	BACKGROUND: #FFFFFF;
	border-bottom:1px dashed #B2B6B9;
}
.tabela tbody td { 
	color:#5A666E;
	padding-left:5px;
}

.tabela tbody td a {  
	color:#333333;
	text-decoration:none;
	font-weight:bold;
 }
.tabela tbody td a:hover { 
	background-color:#469CE3;
	color:#FFF;
 }

.tabela TBODY TR.linhaSalto {
	BACKGROUND:#C6E0FD;
	border-bottom:1px dashed #B2B6B9;
}

.tabela TBODY TR.odd {
	BACKGROUND: #EDF2F8;
	border-bottom:1px dashed #B2B6B9;
}
.tabela tbody tr:hover th, 
.tabela tbody .odd:hover th, 
.tabela tbody tr:hover td {
	background-color:#FF9900; 
	color:#1B486F; 
}

.tabela tr td {
	border-bottom: 1px solid #E5E5E5; 
	border-top: none; 
	border-right: none; 
	border-left: none; 
}


